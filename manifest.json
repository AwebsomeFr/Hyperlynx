{

    "manifest_version": 2,
    "name": "Hyperlynx",
    "version": "0.1",
  
    "description": "Quickly identify links that point to foreign domains.",
  
    "icons": {
      "48": "./hyperlynx-logo.svg",
      "96": "./hyperlynx-logo.svg"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["hyperlynx.js"],
        "css": ["style.css"]
      }
    ]
  
  }