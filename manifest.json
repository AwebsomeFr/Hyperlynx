{

    "manifest_version": 2,
    "name": "Hyperlynx",
    "version": "1.1.0",
  
    "description": "Don't be surprised anymore: Hyperlynx quickly identifies links that point to foreign domains.",
  
    "icons": {
      "48": "./icons/hyperlynx-logo.svg",
      "96": "./icons/hyperlynx-logo.svg"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["hyperlynx.js"],
        "css": ["style.css"]
      }
    ]
  
  }
